	eprint = {1405.5774},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Graphene/Dirac Matrials, T. Wehling, Adv.Phys.63.1-76(2014).pdf:pdf},
	isbn = {0001-8732$\backslash$r1460-6976},
	issn = {14606976},
	journal = {Adv. Phys.},
	keywords = {Dirac materials,back scattering,chirality,d-wave superconductors,graphene,impurity resonance,topological insulators},
	number = {1},
	pages = {1--76},
	title = {{Dirac materials}},
	url = {https://doi.org/10.1080/00018732.2014.927109},
