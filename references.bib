@article{Liu2015PRBCMMP,
	abstract = {Until recently, spin-polarization in nonmagnetic materials was the exclusive territory of low-symmetry crystals, in particular those that lack global inversion symmetry. It was recently shown that a form of hidden spin polarization (named R-2 effect) could exist in centrosymmetric crystals in which individual layers are inversion asymmetric. But other than the abstract notion that such R-2 materials should have layers that belong to polar point group symmetries, there was no practical principle to guide their identification. Here we (i) formulate design principles that indicate ideal R-2 effect; (ii) classify different prototype layered structures that have been broadly studied in the literature for their other functionalities in terms of their expected R-2 behavior, including the Bi2Se3-structure type (a topological insulator), MoS2-structure type (a valleytronic compound) and LaBiOS2-structure type (a superconductor host); (iii) use first-principles band theory to evaluate the specific R-2 design metrics in 17 members of the most promising prototype family of LaOBiS2-type P4/nmm structures. We find large spin splittings (up to 196 meV for holes in LaOBiTe2) as well as characteristic spin textures and surface states. Experimental testing of this prediction is called in terms of their expected R-2 behavior for, and could considerably broaden the range of spintronic crystals.},
	archiveprefix = {arXiv},
	arxivid = {1408.6004},
