	doi = {10.1038/nature25987},
	eprint = {1709.10007},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Dirac Semimetals/Massive Dirac fermions in a ferromagnetic kagome metal, L. Ye, nat.555.638(2018).pdf:pdf},
	issn = {14764687},
