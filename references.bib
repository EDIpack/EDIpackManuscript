@article{Heyl2012JPBAMOP,
	abstract = {We investigate the generation of high-order harmonics using laser pulse energies in the few-$\mu$J range$\backslash$r at high repetition rates. We analyse how the conversion efficiency is influenced by the tight$\backslash$r focusing geometry required for the generation of high-order harmonics under these conditions. A$\backslash$r generalized phase-matching model allows us to discuss macroscopic phase effects independent of focal$\backslash$r length. We present experimental results using the example of a 100 kHz laser system to generate$\backslash$r harmonics up to the 27th order in Ar with a photon flux up to 3 × 10 9 photons s −1 into one$\backslash$r harmonic order. High-repetition-rate femtosecond or even attosecond light sources open new$\backslash$r possibilities for a broad range of applications such as time-resolved photoelectron spectroscopy and$\backslash$r microscopy in the extreme ultraviolet regime.},
