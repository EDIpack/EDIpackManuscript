	eprint = {1801.07957},
	file = {:C$\backslash$:/Users/Nilforoushan/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ronchi et al. - 2018 - Ultrafast orbital manipulation and Mott physics in multi-band correlated materials.pdf:pdf},
	title = {{Ultrafast orbital manipulation and Mott physics in multi-band correlated materials}},
	url = {http://arxiv.org/abs/1801.07957},
	year = {2018},
	bdsk-url-1 = {http://arxiv.org/abs/1801.07957}}

