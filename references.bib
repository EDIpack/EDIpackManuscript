	doi = {10.1073/pnas.1208492109},
	eprint = {1208.0101},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Cuprates and Superconductivity/cuprate-stripes/Optimum inhomogeneity of local lattice distortions in La2CuO4+y-PNAS 109 15685-15690 (2012).pdf:pdf},
	issn = {0027-8424},
	journal = {Proc. Natl. Acad. Sci.},
	number = {39},
	pages = {15685--15690},
	pmid = {22961255},
	title = {{Optimum inhomogeneity of local lattice distortions in La2CuO4+y}},
	url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1208492109},
	volume = {109},
