@article{Das2017,
	abstract = {By combining high-resolution angle resolved photoemission spectroscopy and first principles calculations, we explored the detailed electronic structure of IrO{\$}{\_}2{\$}, a material exhibiting large spin Hall resistivity which makes it particularly suitable as a spin detector. We discuss the role of spin-orbit coupling in the formation of critical bands responsible for its spin Hall properties, namely the evidence of an avoided crossing at the high-symmetry hextuple point along the {\$}{\{}\backslashGamma{\}}{\$} - {\$}{\{}Z{\}}{\$} direction in the Brillouin zone, as identified for the first time experimentally. Our results are in excellent agreement with the recently proposed Dirac nodal lines extending from hextuple point to other critical points in the Brillouin zone [Y. Sun et al, Phys. Rev. B 95, 235104 (2017)], and clearly suggest that spin orbit coupling and band topology plays key role behind the large inverse spin Hall effect of this commercially-widespread centrosymmetric binary oxide.},
