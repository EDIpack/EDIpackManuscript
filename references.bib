	isbn = {1745-2473},
	issn = {17452473},
	journal = {Nat. Phys.},
	number = {8},
	pages = {499--504},
	pmid = {25246403},
	publisher = {Nature Publishing Group},
	title = {{Mapping the orbital wavefunction of the surface states in three-dimensional topological insulators}},
