	journal = {Nature Communications},
	number = {1},
	pages = {7531},
	title = {Mott insulators with boundary zeros},
	url = {https://doi.org/10.1038/s41467-023-42773-7},
	volume = {14},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1038/s41467-023-42773-7}}

