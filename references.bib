	doi = {10.1038/nature02199.Published},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Our work and articles/a-(beta-ttf)2I3/Direct observation of Tomonaga Luttinger liquid state in carbon nanotubes at low temperatures.pdf:pdf},
	issn = {1476-4687},
	journal = {Nature},
	number = {December},
	pages = {540},
