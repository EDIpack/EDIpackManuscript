	doi = {10.1038/nature04233},
	eprint = {0509330},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Graphene/Two dimensional gas of massless Dirac fermions in graphene, K. S. Novoselov, Nat.438.197(2005).pdf:pdf},
	isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
	issn = {00280836},
	journal = {Nature},
	number = {7065},
	pages = {197--200},
	pmid = {16281030},
	primaryclass = {cond-mat},
	title = {{Two-dimensional gas of massless Dirac fermions in graphene}},
	volume = {438},
	year = {2005},
	bdsk-url-1 = {https://doi.org/10.1038/nature04233}}

@article{Berger1995PRB,
