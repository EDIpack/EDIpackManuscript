	journal = {Scientific Reports},
	number = {1},
	pages = {4154},
	title = {Topological edge states in high-temperature superconductiving {FeSe/SrTiO}$_3$ films with {T}e substitution},
	ty = {JOUR},
	url = {https://doi.org/10.1038/s41598-019-40644-0},
	volume = {9},
	year = {2019},
