	journal = {Nat. Commun.},
	pages = {1--6},
	title = {{Signature of type-II Weyl semimetal phase in MoTe$_2$}},
	volume = {8},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1038/ncomms13973}}

