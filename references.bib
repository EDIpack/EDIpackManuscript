	doi = {10.1103/PhysRevB.91.224509},
	eprint = {1506.00070},
	file = {:C$\backslash$:/Users/Nilforoushan/Desktop/PhD files/PhD/Bibilography-PhD/Cuprates and Superconductivity/Quasiparticle dynamics in high-temperature superconductors far from equilibrium-C.Piovera-PhysRevB.91.224509(2015).pdf:pdf},
	issn = {1550235X},
	journal = {Phys. Rev. B - Condens. Matter Mater. Phys.},
	number = {22},
	pages = {1--7},
	title = {{Quasiparticle dynamics in high-temperature superconductors far from equilibrium: An indication of pairing amplitude without phase coherence}},
