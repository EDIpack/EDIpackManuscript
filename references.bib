@article{Keer1976JSSC,
	abstract = {Heat capacities have been measured for single crystals of V2O3, either pure or doped with 1 and 1.4 mole{\%} Cr2O3 and Al2O3 over the temperature range 100--700$\,^{\circ}$K. V2O3 undergoes a fairly sharp transition at low temperatures (∼170$\,^{\circ}$K) but fails to exhibit any thermal anomaly above 300$\,^{\circ}$K. The thermal behavior of (MxV1−x)2O3, M = Cr, Al, is manifested by two transitions: one at low temperatures, 170--180$\,^{\circ}$K for x = 0.01 and 180--190$\,^{\circ}$K for x = 0.014, and the other at high temperatures. For x = 0.01, the high-temperature (H-T) anomaly extended over the range 325--345$\,^{\circ}$K (Cr-doped V2O3) and 345--365$\,^{\circ}$K (Al-doped V2O3), respectively. The corresponding ranges for x = 0.014 were found to be 260--280$\,^{\circ}$K and 270--290$\,^{\circ}$K, respectively. Further, the H-T anomaly was characterized by a large hysteresis (∼50$\,^{\circ}$K). The values of lattice heat capacity of pure and doped V2O3 were, however, found to be almost the same and could be empirically represented by the Debye (D)−Einstein (E) function with $\theta$ values 430$\,^{\circ}$K (T = 100--230$\,^{\circ}$K) and 465$\,^{\circ}$K (T {\textgreater} 230$\,^{\circ}$K), respectively. Further, the enthalpy change $\Delta$H associated with the H-T anomaly in doped V2O3 (80 ≤ $\Delta$H ≤ 510 J/mole) was 5--10 times smaller than the $\Delta$H corresponding to the lower-temperature transition. The results cited here appear incompatible with the Mott transition model that has been invoked to explain the H-T anomaly.},
	author = {Keer, H.V. and Dickerson, D.L. and Kuwamoto, H. and Barros, H.L.C. and Honig, J.M.},
	date-added = {2021-04-14 16:40:02 +0200},
