@article{Konig2007S,
	annote = {10.1126/science.1148047},
	author = {K{\"o}nig, Markus and Wiedmann, Steffen and Br{\"u}ne, Christoph and {\textit{ et al.}}},
	date = {2007/11/02},
	date-added = {2021-04-14 16:40:02 +0200},
	date-modified = {2021-04-30 15:51:19 +0200},
	doi = {10.1126/science.1148047},
	journal = {Science},
	m3 = {10.1126/science.1148047},
	month = {11},
	number = {5851},
	pages = {766--770},
	title = {{Quantum Spin Hall Insulator State in HgTe Quantum Wells}},
	ty = {JOUR},
	url = {http://www.sciencemag.org/content/318/5851/766.abstract N2 - Recent theory predicted that the quantum spin Hall effect, a fundamentally new quantum state of matter that exists at zero external magnetic field, may be realized in HgTe/(Hg,Cd)Te quantum wells. We fabricated such sample structures with low density and high mobility in which we could tune, through an external gate voltage, the carrier conduction from n-type to p-type, passing through an insulating regime. For thin quantum wells with well width d < 6.3 nanometers, the insulating regime showed the conventional behavior of vanishingly small conductance at low temperature. However, for thicker quantum wells (d > 6.3 nanometers), the nominally insulating regime showed a plateau of residual conductance close to 2e2/h, where e is the electron charge and h is Planck's constant. The residual conductance was independent of the sample width, indicating that it is caused by edge states. Furthermore, the residual conductance was destroyed by a small external magnetic field. The quantum phase transition at the critical thickness, d = 6.3 nanometers, was also independently determined from the magnetic field--induced insulator-to-metal transition. These observations provide experimental evidence of the quantum spin Hall effect.},
	volume = {318},
	year = {2007},
	bdsk-url-1 = {http://dx.doi.org/10.1126/science.1148047},
	bdsk-url-2 = {http://www.sciencemag.org/content/318/5851/766.abstract%20N2%20-%20Recent%20theory%20predicted%20that%20the%20quantum%20spin%20Hall%20effect,%20a%20fundamentally%20new%20quantum%20state%20of%20matter%20that%20exists%20at%20zero%20external%20magnetic%20field,%20may%20be%20realized%20in%20HgTe/(Hg,Cd)Te%20quantum%20wells.%20We%20fabricated%20such%20sample%20structures%20with%20low%20density%20and%20high%20mobility%20in%20which%20we%20could%20tune,%20through%20an%20external%20gate%20voltage,%20the%20carrier%20conduction%20from%20n-type%20to%20p-type,%20passing%20through%20an%20insulating%20regime.%20For%20thin%20quantum%20wells%20with%20well%20width%20d%20%3C%206.3%20nanometers,%20the%20insulating%20regime%20showed%20the%20conventional%20behavior%20of%20vanishingly%20small%20conductance%20at%20low%20temperature.%20However,%20for%20thicker%20quantum%20wells%20(d%20%3E%206.3%20nanometers),%20the%20nominally%20insulating%20regime%20showed%20a%20plateau%20of%20residual%20conductance%20close%20to%202e2/h,%20where%20e%20is%20the%20electron%20charge%20and%20h%20is%20Planck's%20constant.%20The%20residual%20conductance%20was%20independent%20of%20the%20sample%20width,%20indicating%20that%20it%20is%20caused%20by%20edge%20states.%20Furthermore,%20the%20residual%20conductance%20was%20destroyed%20by%20a%20small%20external%20magnetic%20field.%20The%20quantum%20phase%20transition%20at%20the%20critical%20thickness,%20d%20=%206.3%20nanometers,%20was%20also%20independently%20determined%20from%20the%20magnetic%20field--induced%20insulator-to-metal%20transition.%20These%20observations%20provide%20experimental%20evidence%20of%20the%20quantum%20spin%20Hall%20effect.}}

