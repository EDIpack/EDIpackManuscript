	journal = {Surf. Interface Anal.},
	number = {1},
	pages = {2--11},
	title = {{Quantitative electron spectroscopy of surfaces: A standard data base for electron inelastic mean free paths in solids}},
	volume = {1},
	year = {1979},
	bdsk-url-1 = {https://doi.org/10.1002/sia.740010103}}

